#logo {
    position:absolute;
    left:10px;
    top:5px;
}

html, body {
    height: 100%;
    font: 67.5% "Lucida Sans Unicode", "Bitstream Vera Sans", "Trebuchet Unicode MS", "Lucida Grande", Verdana, Helvetica, sans-serif;
}
body{
	min-height: 100%;
}
.kibana-container{
    margin: auto;
    padding: 35px 0px 0px 0px; 
    text-align:left;
    width: 100%;
    height: 2500px;
    border: 0px solid rgb(20,240,240);
    padding-bottom: 8em;	
}
#mainMenu{
    border:none;
    border:0px;
    margin:0px;
    margin-top:-8px;
    padding:0px;
    font: 67.5% "Lucida Sans Unicode", "Bitstream Vera Sans", "Trebuchet Unicode MS", "Lucida Grande", Verdana, Helvetica, sans-serif;
    font-size:14px;
    font-weight:bold;
    position: fixed;
    width:100%;
    z-index:100;
}

#mainMenu ul{
    background: #C3FFED;
    height:60px;
    list-style:none;
    margin:0;
    border-bottom: 1px solid rgb(0,209,167);
    /*margin-top:-8px;*/
    margin-left:-8px;
    padding:0;
}

#mainMenu li{
    float:left;
    padding:0px;
}

#mainMenu li a{
    /*background: rgb(109,109,109);*/
    color: rgb(71,71,71);  /* 메인메뉴 색깔 */
    display:block;
    font-weight:normal;
    line-height:50px;
    margin:0px;
    padding:5px 25px;
    text-align:center;
    text-decoration:none;
}
/* 메인메뉴 호버시 */
#mainMenu li a:hover, .menubar ul li:hover a{
    background: rgb(0,209,167);
    color:#FFFFFF;
    text-decoration:none;
}

#mainMenu li ul{
    background: rgb(176,245,214);
    display:none; /* 평상시에는 드랍메뉴가 안보이게 하기 */
    height:auto;
    padding:0px;
    margin:0px;
    border:0px;
    position:absolute;
    width:150px;
    z-index:200;
    /*top:1em;
     */*left:0;*/
 }

 #mainMenu li:hover ul{
     display:block; /* 마우스 커서 올리면 드랍메뉴 보이게 하기 */
 }

 #mainMenu li li {
     background: rgb(176,245,214);
     display:block;
     float:none;
     margin:0px;
     padding:0px;
     width:150px;
 }

 #mainMenu li:hover li a{
     background:none;
 }

 #mainMenu li ul a{
     display:block;
     height:50px;
     font-size:12px;
     font-style:normal;
     margin:0px;
     padding:0px 10px 0px 15px;
     text-align:left;
 }
 /* 서브메뉴 호버시 */
 #mainMenu li ul a:hover, .menubar li ul li:hover a{
     background: rgb(0,209,167);
     border:0px;
     color:#ffffff;
     text-decoration:none;
 }

 #mainMenu p{
     clear:left;
 }

 /* 검색 섹션 */
 #searchSection {
     margin-left:12%;
     display:inline;
 }
 #searchBox {
     font-size: 14px;
     margin-top: 18px;
     height: 20px;
     text-align:center;
 }
 #searchButton{
     background-color:white;
     font-size: 14px;
     margin-top: 18px;
     height: 23px;
     width: 45px;
 }
 #searchButton:hover {
     background-color: rgb(0,209,167);
     color: white;
     border: 1px solid rgb(0,209,167);
 }

 /* 로그인 섹션 */
 #loginSection {
     float:right;
     margin-top: 18px;
     padding-right:35px;
     color:rgb(71,71,71);
     font-weight: normal;
 }

 #loginSection input {
     border: 1px solid rgb(150,150,150);
     height:18px;
     width:80px;
     font-size:14px;
 }

 #loginSection #login {
     background-color:white;
     border:1px solid rgb(170,170,170);
     height:23px;
     width:60px;
     font-size:13px;
     color:rgb(71,71,71);
 }

 #loginSection #login:hover {
     background-color: rgb(0,209,167);
     color:white;
     border:1px solid rgb(0,209,167);
 }

 #loginSection #register {
     background-color:white;
     border:1px solid rgb(170,170,170);
     height:23px;
     width:65px;
     font-size:13px;
     color:rgb(71,71,71);
 }

 #loginSection #register:hover {
     background-color: rgb(255,180,66);
     color:white;
     border:1px solid rgb(255,180,66);
 }


 #backgroundBlock {
     height: 51px;
     margin-bottom: 5px;
 }

 .flipped {
     transform: scale(-1, 1);
     -moz-transform: scale(-1, 1);
     -webkit-transform: scale(-1, 1);
     -o-transform: scale(-1, 1);
     -khtml-transform: scale(-1, 1);
     -ms-transform: scale(-1, 1);
 }

 /* REGISTRATION FORM */
 #closeRegistrationForm {
     float:right;
     margin-right:5px;
     background-color: white;
     border: 1px solid rgb(130,130,130);
     text-decoration: none;
     font: 15px "Lucida Sans Unicode", "Bitstream Vera Sans", "Trebuchet Unicode MS", "Lucida Grande", Verdana, Helvetica, sans-serif;
 }

 #closeRegistrationForm:hover {
     color: white;
     background-color:  rgb(0,209,167);
     border: 1px solid  rgb(0,209,167);
     text-decoration: none;
     font: 15px "Lucida Sans Unicode", "Bitstream Vera Sans", "Trebuchet Unicode MS", "Lucida Grande", Verdana, Helvetica, sans-serif;
 }

 /* 약관 상자 */
 #contractBox {
     margin-left:3px;
     background-color: white;
     width: 330px;
     height: 80px;
     font-size:11px;
     border:0px solid white;
 }

 #registrationForm {
     background-color: rgb(240,240,240);
     color: black;
     padding: 5px 5px 5px 5px;
     font: 16px "Lucida Sans Unicode", "Bitstream Vera Sans", "Trebuchet Unicode MS", "Lucida Grande", Verdana, Helvetica, sans-serif;
 }

 .noclose .ui-dialog-titlebar-close
 {
     display:none;
 }

 input {
     border: 1px solid rgb(150,150,150);
 }

#center {
    width:100%;
    text-align:center;
    min-height:100%;
}

 #contentBody {
     margin: 0 auto;
     text-align:left;
     width: 1280px;
     height:1280px;
     border: 1px solid rgb(240,240,240);
     padding-bottom:8em;
 }

 #foot {
     padding-top:7px;
     width: 1280px;
     z-index:100;
     font-size:16px;
     margin-top:-7.6em;
     margin-bottom:-20px;
     height:8em;
     background-color: rgb(230,230,230);
 }

